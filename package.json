{
	"name": "senpai",
	"version": "1.0.0",
	"description": "Telegram bot for managing the 4bis1 group.",
	"main": "bin/www",
	"scripts": {
		"start": "node bin/www",
		"development": "nodemon bin/www",
		"test": "npm run format:check",
		"format": "prettier --write --check \"**/*.{js,json,md}\"\n",
		"format:check": "prettier --check \"**/*.{js,json,md}\"\n",
		"update": "yarn update"
	},
	"bin": {
		"senpai": "www"
	},
	"engines": {
		"node": ">=14.0.0",
		"npm": ">=6.14.5",
		"yarn": ">=1.22.0"
	},
	"keywords": [
		"Telegram",
		"Bot",
		"Helper",
		"Assistant"
	],
	"author": "Genemator Sakhib <genemators@gmail.com> (https://genemator.me)",
	"license": "CC0-1.0",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/4bis1/senpai.git"
	},
	"bugs": {
		"url": "https://github.com/4bis1/senpai/issues"
	},
	"homepage": "https://github.com/4bis1/senpai#readme",
	"dependencies": {
		"axios": "0.20.0",
		"bad-words": "^3.0.3",
		"colors": "^1.4.0",
		"dotenv": "^8.2.0",
		"is-reachable": "^5.0.0",
		"node-cron": "^2.0.3",
		"nodemon": "^2.0.4",
		"prettier": "^2.1.1",
		"telegraf": "^3.38.0"
	}
}
